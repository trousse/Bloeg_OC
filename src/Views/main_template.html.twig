{% extends "_index.html.twig" %}

{% block title %}{{ title }}{% endblock %}

{% block head %}
{{ parent() }}
    {% for script in scripts %}
        <script src={{ script }} ></script>
    {% endfor %}
    {% for style in styles %}
        <link rel="stylesheet" href={{ style }} />
    {% endfor %}
{% endblock %}

{% block content %}
    {{include('_header.html.twig')}}
        <div id="page_container">
            {% block page %}

            {% endblock %}
        </div>
    {{include('_footer.html.twig')}}
{% endblock %}


{% block footer %}
    {{ parent() }}
{% endblock %}